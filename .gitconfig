[user]
  name = yuxxeun
  email = itsyuxxeun@gmail.com
  signkey = -

[core]
  pager = delta --plus-color="#012800" --minus-color="#340001" --theme='Monokai Extended'
  excludesfile = ~/.config/git/gitignore

[interactive]
  diffFilter = delta --color-only

[commit]
  gpgsign = false

[includeIf "gitdir:~/Developer/headsynclabs/"]
  path = ~/.config/git/headsynclabs

[credential]
  helper = store

[difftool]
  prompt = true

[init]
  defaultBranch = main
